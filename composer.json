{
  "name": "imerit-pro/gateway",
  "homepage": "https://github.com/imerit-pro/gateway",
  "description": "A Laravel package for connecting to all Saman payment  gateway",
  "keywords": [
    "laravel",
    "iran",
    "payment",
    "gateway",
    "port",
    "bank",
    "pay",
    "saman",
    "paypal"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Med Ga",
      "email": "amdeadman@gmail.com"
    }
  ],
  "require": {
    "nesbot/carbon": "*",
    "paypal/rest-api-sdk-php": "*"
  },
  "autoload": {
    "psr-4": {
      "Imerit\\Gateway\\": "src/",
      "Imeirt\\Gateway\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "Imerit\\Gateway\\GatewayServiceProvider"
      ],
      "aliases": {
        "Gateway": "Imerit\\Gateway\\Gateway"
      }
    }
  }
}
